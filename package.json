{
  "name": "signux",
  "version": "1.0.0",
  "module": "index.ts",
  "type": "module",
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/bun": "latest",
    "eslint": "^9.24.0",
    "globals": "^16.0.0",
    "typescript-eslint": "^8.29.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./operators": {
      "import": "./dist/operators/index.js",
      "types": "./dist/operators/index.d.ts"
    }
  },
  "scripts": {
    "build": "bun ./scripts/build.ts && tsc --declaration --emitDeclarationOnly --noEmit false --outDir dist",
    "prepublishOnly": "bun run build",
    "lint": "eslint .",
    "ts:check": "tsc"
  }
}
